<%- include("header") %>

<!-- MAIN CONTENT-->
<div class="main-content">
  <div class="section__content section__content--p30">
    <div class="container-fluid">
      <div class="row">
        <div class="col-lg-12">
          <div class="table-responsive table--no-card m-b-30">
            <table class="table table-borderless table-striped table-earning">
              <thead>
                <tr>
                  <th>Category Name</th>
                  <th>Title</th>
                  <th>Description</th>
                  <th>Image</th>
                  <th>Action</th>
                </tr>
              </thead>
              <tbody>
                <% for (let v of subcategorydata) { %>
                <tr>
                  <td><%= v.subid.title %></td>
                  <td><%= v.title %></td>
                  <td><%= v.description %></td>
                  <td><img src="<%= v.subcategoryimage %>" height="100"></td>
                  <td style="font-size: 25px;">
                    <a href="/admin/subcategory/deleteSubcategoryRecord/<%= v.id %>"><i class="fa fa-trash"></i></a>
                  </td>
                </tr>
                <% } %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<%- include("footer") %>
