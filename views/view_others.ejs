<%- include("header") %>

    <!-- MAIN CONTENT-->
    <div class="main-content">
        <div class="section__content section__content--p30">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="table-responsive table--no-card m-b-30">
                            <table class="table table-borderless table-striped table-earning">
                                <thead>
                                    <tr>
                                        <th>Name</th>
                                        <th>Country</th>
                                        <th>City</th>
                                        <th>description</th>
                                       
                                    </tr>
                                </thead>
                                <tbody>
                                    <% var no=1; for(var v of othersdata) { %>
                                        <tr>
                                            <td>
                                                <%= v.name %>
                                            </td>
                                            <td>
                                                <%= v.country %>
                                            </td>
                                            <td>
                                                <%= v.city %>
                                            </td>
                                            <td>
                                                <%= v.description %>
                                            </td>
                                           
                                           
                                            <td style="font-size: 25px;">
                                                <a href="/admin/others/deleteOtherRecord/<%= v.id %>"><i class="fa fa-trash"></i></a>
                                               
                                            </td>

                                        </tr>
                                        <% } %>
                                </tbody>
                            </table>
                        </div>
                    </div>

                </div>






                <%- include("footer") %>